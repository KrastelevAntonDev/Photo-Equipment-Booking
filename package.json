{
  "name": "photo-equipment-booking",
  "version": "1.0.0",
  "description": "Backend project for booking photo studios and equipment written in node js express, mongo db",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register src/seed/admin.seed.ts",
    "seed:equipment": "ts-node -r tsconfig-paths/register src/seed/equipment.seed.ts",
    "seed:equipment-quantity": "ts-node -r tsconfig-paths/register src/seed/update-equipment-quantity.seed.ts",
    "seed:images": "ts-node -r tsconfig-paths/register src/seed/add-images.seed.ts",
    "seed:rooms": "ts-node -r tsconfig-paths/register src/seed/room.seed.ts",
    "seed:rooms-pricing": "ts-node -r tsconfig-paths/register src/seed/rooms.pricing.seed.ts",
    "seed:remove-fri-pricing": "ts-node -r tsconfig-paths/register src/seed/remove-fri-pricing.seed.ts",
    "seed:rename-equipment-price": "ts-node -r tsconfig-paths/register src/seed/rename-equipment-price-field.seed.ts",
    "seed:set-rooms-available": "ts-node -r tsconfig-paths/register src/seed/set-rooms-available.seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrastelevAntonDev/Photo-Equipment-Booking.git"
